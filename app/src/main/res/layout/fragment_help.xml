<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.HelpFragment"
    android:id="@+id/simpleFrameLayout"
    >

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/simpleTabLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:contentDescription="tab layout"
        android:backgroundTint="@color/beak_gold"
        app:tabBackground="@color/beak_gold"
        app:tabIndicatorColor="#755700"
        app:tabSelectedTextColor="#755700"
        app:tabTextColor="@color/wingtip_black"
        app:tabTextAppearance="@style/TextAppearance.AppCompat.Small"
        >

    </com.google.android.material.tabs.TabLayout>

